    ],
    "checkver": {
        "url": "https://github.com/cemu-project/Cemu/releases",
        "regex": "Cemu (\\d+(\\.\\d+)*(-\\d+)?)"
    },
    "autoupdate": {
        "url": "https://github.com/cemu-project/Cemu/releases/download/v$version/cemu-$version-windows-x64.zip",
        "extract_dir": "Cemu_$version"
    }
}
